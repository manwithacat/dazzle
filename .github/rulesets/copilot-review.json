{
  "name": "copilot-code-review",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "include": [
        "~DEFAULT_BRANCH"
      ],
      "exclude": []
    }
  },
  "rules": [
    {
      "type": "copilot_code_review",
      "parameters": {
        "review_draft_pull_requests": true,
        "review_on_push": true
      }
    }
  ],
  "bypass_actors": []
}
