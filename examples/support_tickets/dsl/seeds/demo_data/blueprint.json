{
  "version": "1.0",
  "blueprint": {
    "project_id": "support_tickets",
    "domain_description": "Customer support ticketing system for managing support requests and resolutions",
    "seed": 42,
    "tenants": [
      {
        "name": "Alpha Services Ltd",
        "slug": "alpha-services-ltd",
        "notes": "Demo tenant 1"
      },
      {
        "name": "Bravo Services Ltd",
        "slug": "bravo-services-ltd",
        "notes": null
      }
    ],
    "personas": [
      {
        "persona_name": "Staff",
        "description": "Regular staff users",
        "default_role": "role_staff",
        "default_user_count": 3
      }
    ],
    "entities": [
      {
        "name": "User",
        "row_count_default": 0,
        "notes": "User",
        "tenant_scoped": false,
        "field_patterns": [
          {
            "field_name": "id",
            "strategy": "uuid_generate",
            "params": {}
          },
          {
            "field_name": "email",
            "strategy": "email_from_name",
            "params": {
              "source_field": "full_name",
              "domains": [
                "example.test"
              ]
            }
          },
          {
            "field_name": "name",
            "strategy": "person_name",
            "params": {
              "locale": "en_GB"
            }
          },
          {
            "field_name": "role",
            "strategy": "enum_weighted",
            "params": {
              "enum_values": [
                "customer",
                "agent",
                "manager"
              ],
              "weights": [
                0.3333333333333333,
                0.3333333333333333,
                0.3333333333333333
              ]
            }
          },
          {
            "field_name": "is_active",
            "strategy": "boolean_weighted",
            "params": {
              "true_weight": 0.3
            }
          },
          {
            "field_name": "created_at",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0
            }
          }
        ]
      },
      {
        "name": "Ticket",
        "row_count_default": 20,
        "notes": "Support Ticket",
        "tenant_scoped": false,
        "field_patterns": [
          {
            "field_name": "id",
            "strategy": "uuid_generate",
            "params": {}
          },
          {
            "field_name": "ticket_number",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 2,
              "max_words": 5
            }
          },
          {
            "field_name": "title",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 3,
              "max_words": 8
            }
          },
          {
            "field_name": "description",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 5,
              "max_words": 20
            }
          },
          {
            "field_name": "status",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0,
              "enum_values": [
                "open",
                "in_progress",
                "resolved",
                "closed"
              ],
              "weights": [
                0.25,
                0.25,
                0.25,
                0.25
              ]
            }
          },
          {
            "field_name": "priority",
            "strategy": "enum_weighted",
            "params": {
              "enum_values": [
                "low",
                "medium",
                "high",
                "critical"
              ],
              "weights": [
                0.25,
                0.25,
                0.25,
                0.25
              ]
            }
          },
          {
            "field_name": "category",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0,
              "enum_values": [
                "bug",
                "feature",
                "question",
                "other"
              ],
              "weights": [
                0.25,
                0.25,
                0.25,
                0.25
              ]
            }
          },
          {
            "field_name": "created_by",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0
            }
          },
          {
            "field_name": "assigned_to",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 2,
              "max_words": 5
            }
          },
          {
            "field_name": "resolution",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 2,
              "max_words": 5
            }
          },
          {
            "field_name": "created_at",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0
            }
          },
          {
            "field_name": "updated_at",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0
            }
          },
          {
            "field_name": "resolved_at",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0
            }
          }
        ]
      },
      {
        "name": "Comment",
        "row_count_default": 20,
        "notes": "Comment",
        "tenant_scoped": false,
        "field_patterns": [
          {
            "field_name": "id",
            "strategy": "uuid_generate",
            "params": {}
          },
          {
            "field_name": "ticket",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 2,
              "max_words": 5
            }
          },
          {
            "field_name": "author",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 2,
              "max_words": 5
            }
          },
          {
            "field_name": "content",
            "strategy": "free_text_lorem",
            "params": {
              "min_words": 2,
              "max_words": 5
            }
          },
          {
            "field_name": "is_internal",
            "strategy": "boolean_weighted",
            "params": {
              "true_weight": 0.3
            }
          },
          {
            "field_name": "created_at",
            "strategy": "date_relative",
            "params": {
              "anchor": "today",
              "min_offset_days": -365,
              "max_offset_days": 0
            }
          }
        ]
      }
    ]
  }
}
