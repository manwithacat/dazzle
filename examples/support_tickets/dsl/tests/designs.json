      "test_id": "SM_TICKET_001",
      "title": "State machine transitions for Ticket",
      "description": "Verify all valid state transitions for Ticket",
      "persona": null,
      "scenario": null,
      "trigger": "user_click",
      "steps": [
        {
          "action": "create",
          "target": "entity:Ticket",
          "data": {
            "status": "open"
          },
          "rationale": "Create Ticket in 'open' state"
        },
        {
          "action": "trigger_transition",
          "target": "entity:Ticket",
          "data": {
            "from_state": "open",
            "to_state": "in_progress"
          },
          "rationale": "Transition from 'open' to 'in_progress'"
        },
        {
          "action": "create",
          "target": "entity:Ticket",
          "data": {
            "status": "in_progress"
          },
          "rationale": "Create Ticket in 'in_progress' state"
        },
        {
          "action": "trigger_transition",
          "target": "entity:Ticket",
          "data": {
            "from_state": "in_progress",
            "to_state": "resolved"
          },
          "rationale": "Transition from 'in_progress' to 'resolved'"
        },
        {
          "action": "create",
          "target": "entity:Ticket",
          "data": {
            "status": "in_progress"
          },
          "rationale": "Create Ticket in 'in_progress' state"
        },
        {
          "action": "trigger_transition",
          "target": "entity:Ticket",
          "data": {
            "from_state": "in_progress",
            "to_state": "open"
          },
          "rationale": "Transition from 'in_progress' to 'open'"
        }
      ],
      "expected_outcomes": [
        "All valid transitions complete successfully",
        "Final state matches expected state"
      ],
      "entities": [
        "Ticket"
      ],
