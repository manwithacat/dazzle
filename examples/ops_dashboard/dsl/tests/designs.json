      "title": "State machine transitions for System",
      "description": "Verify all valid state transitions for System",
      "persona": null,
      "scenario": null,
      "trigger": "user_click",
      "steps": [
        {
          "action": "create",
          "target": "entity:System",
          "data": {
            "status": "healthy"
          },
          "rationale": "Create System in 'healthy' state"
        },
        {
          "action": "trigger_transition",
          "target": "entity:System",
          "data": {
            "from_state": "healthy",
            "to_state": "degraded"
          },
          "rationale": "Transition from 'healthy' to 'degraded'"
        },
        {
          "action": "create",
          "target": "entity:System",
          "data": {
            "status": "healthy"
          },
          "rationale": "Create System in 'healthy' state"
        },
        {
          "action": "trigger_transition",
          "target": "entity:System",
          "data": {
            "from_state": "healthy",
            "to_state": "critical"
          },
          "rationale": "Transition from 'healthy' to 'critical'"
        },
        {
          "action": "create",
          "target": "entity:System",
          "data": {
            "status": "degraded"
          },
          "rationale": "Create System in 'degraded' state"
        },
        {
          "action": "trigger_transition",
          "target": "entity:System",
          "data": {
            "from_state": "degraded",
            "to_state": "healthy"
          },
          "rationale": "Transition from 'degraded' to 'healthy'"
        }
      ],
      "expected_outcomes": [
        "All valid transitions complete successfully",
        "Final state matches expected state"
      ],
