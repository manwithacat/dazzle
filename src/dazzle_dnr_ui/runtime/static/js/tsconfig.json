{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "skipLibCheck": true
  },
  "include": ["./**/*.js"],
  "exclude": ["./**/*.test.js"]
}
